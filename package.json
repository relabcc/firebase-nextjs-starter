{
  "name": "firebase-functions-nextjs",
  "version": "2.0.0",
  "main": "index.js",
  "license": "MIT",
  "dependencies": {
    "@babel/runtime": "^7.1.2",
    "clean-tag": "^2.0.2",
    "express": "^4.16.4",
    "firebase": "^5.5.5",
    "firebase-admin": "^6.0.0",
    "firebase-functions": "^2.0.5",
    "immutable": "^4.0.0-rc.12",
    "lodash": "^4.17.11",
    "next": "6.1.2",
    "open-color": "^1.6.3",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "react-helmet": "^5.2.0",
    "react-measure": "^2.1.2",
    "react-redux": "^5.0.7",
    "react-redux-firebase": "^2.1.8",
    "redux": "^4.0.1",
    "redux-actions": "^2.6.4",
    "redux-firestore": "^0.5.8",
    "redux-immutable": "^4.0.0",
    "redux-responsive": "^4.3.8",
    "reselect": "^4.0.0",
    "sanitize.css": "^8.0.0",
    "styled-components": "3.4.10",
    "styled-system": "^3.1.11"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0-beta.42",
    "@babel/core": "^7.0.0-beta.42",
    "@babel/preset-env": "^7.0.0-beta.42",
    "@firebase/app-types": "^0.3.2",
    "cpx": "^1.5.0",
    "firebase-tools": "^5.1.1",
    "rimraf": "^2.6.2"
  },
  "scripts": {
    "fblogin": "yarn firebase login",
    "start": "yarn dev",
    "dev": "node src/app/server",
    "preserve": "yarn build-public && yarn build-funcs && yarn build-app && yarn copy-deps && yarn install-deps",
    "serve": "NODE_ENV=production yarn firebase serve --only functions,hosting",
    "deploy-app": "yarn firebase deploy --only hosting,functions:app",
    "deploy-all": "yarn firebase deploy",
    "clean": "rimraf \"dist/functions\" && rimraf \"dist/public\"",
    "build-public": "cpx \"src/public/**/*.*\" \"dist/public\" -C",
    "build-funcs": "babel \"src/functions\" --out-dir \"dist/functions\"",
    "build-app": "NODE_ENV=production next build \"src/app\"",
    "copy-deps": "cpx \"*{package.json,yarn.lock}\" \"dist/functions\"",
    "install-deps": "cd \"dist/functions\" && yarn"
  }
}
